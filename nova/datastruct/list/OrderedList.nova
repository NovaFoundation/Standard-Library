package "nova/datastruct/list"

[AutoFinal]
trait OrderedList<Type> {
    public findIndex(condition(Type, Int, OrderedList<Type>) -> Bool, Int defaultReturnValue = -1) -> Bool => defaultReturnValue {
        var Int i = 0
        
        let list = (List)this
        
        for (element in list) {
            if (condition(element, i++, this)) {
                return i
            }
        }
    }
}