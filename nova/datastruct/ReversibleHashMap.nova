package "nova/datastruct"

class ReversibleHashMap<K, V> extends HashMap<K, V> {
    HashMap<V, K> rev
    
    public construct() {
        rev = new HashMap()
    }
    
    public put(K key, V value) {
        /*super.put(key, value)
        rev.put(value, key)*/
    }
    
    public getKey(V value) => rev.get(value)
    public getValue(K key) => get(key)
}