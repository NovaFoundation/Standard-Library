package "nova/star"

class Label extends UIComponent {
    public String text
    
    public construct(public String text, passing Int x, passing Int y) =>
        super(x, y)
    
    public draw() {
        external c {
            TextOut(#{window.hdc}, #{absoluteX}, #{absoluteY}, #{text.chars.data}, #{text.count});
        }
    }
}