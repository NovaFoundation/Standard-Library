package "nova/math"

class Vector2D extends Vector
{
	public Double x => data[0]
	{
		set { data[0] = value }
	}
	
	public Double y => data[1]
	{
		set { data[1] = value }
	}
	
	public construct()
	{
		super(2)
	}
	
	public dotProduct(Vector3D other) -> Double
	{
		return super.dotProduct(other)
	}
}