package "nova/primitive/number"

import "nova/datastruct/Comparable"

public class Byte extends Number implements Comparable<Byte>, Integer
{
	Byte value
	
	construct(Byte value)
	{
		this.value = value
	}
	
	public static numDigits(Byte number) -> Int
	{
		return Long.numDigits(number)
	}
	
	public static toString(Byte value) -> String
	{
		return Long.toString(value)
	}
	
	public toString() -> String
	{
		return toString(value)
	}
	
